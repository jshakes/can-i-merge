{
	"name": "Can I merge?",
	"version": "0.1",
	"description": "Shows at a glance which of your PRs are ready to merge",
	"manifest_version": 2,
	"content_scripts": [
		 {
			 "matches": [
			 		"https://*.github.com/*/pulls",
			 		"https://*.github.com/*/pulls*",
			 		"https://*.github.com/pulls",
			 		"https://*.github.com/pulls*"
			 	],
			 "js": ["contentScript.js"],
			 "css": ["contentStyles.css"]
		 }
	 ],
	"permissions": ["activeTab"]
}